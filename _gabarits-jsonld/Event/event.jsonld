{
   "@context":"http://schema.org/",
   "@type":"Event",
   "@id":"--URI constituant un identifiant unique de l'événement. Optionnel--",
   "additionalType":"--URI d'une catégorie correspondant au type particulier de l'événement, par exemple http://kg.artsdata.ca/resource/PerformingArtsEvent. Voir la liste des catégories sur http://kg.artsdata.ca/resource/ArtsdataEventTypes--",
   "name":"--Titre de l'événement--",
   "startDate":"--Date et heure du début de l'événement, en format ISO 8601, par exemple 2022-03-15T08:30:00:00-05:00--",
   "endDate":"--Date et heure de fin de l'événement, en format ISO 8601. Si l'heure n'est pas connue, mettez  seulement la date, par exemple 2022-03-15--",
   "eventStatus":"https://schema.org/EventScheduled",
   "eventAttendanceMode":"https://schema.org/OfflineEventAttendanceMode",
   "description":"--Description de l'événement--",
   "url":"--URL de la page Web de l'événement--",
   "duration":"--Durée de l'événement en format ISO 8601, par exemple : PT1H30M --",
   "image":"--URL de l'image--",
   "organizer":{
      "@type":"Organization",
      "name":"--Nom du diffuseur--",
      "url":"--URL de site ou de la page officielle du diffuseur--",
      "sameAs":[
         "--lien vers un ou des comptes de réseaux sociaux du diffuseur, si disponible--",
         "--URI Artsdata.ca, obtenu avec l'API de réconciliation Artsdata--",
         "--URI Wikidata.org, si disponible--"]
      },
   "performer":[{
      "@type":"--Utiliser le type 'Organization' si l'interprète est une compagnie ou un groupe--",
      "@id":"--URI constituant un identifiant unique de la compagnie. Optionnel--",
      "name":"--Nom de la compagnie--",
      "url":"--URL du site officiel de la compagnie--",
      "sameAs":[
         "--lien vers un ou des comptes de réseaux sociaux, si disponible--",
         "--URI Artsdata.ca, obtenu avec l'API de réconciliation Artsdata--",
         "--URI wikidata, si disponible--"]
   },{
      "@type":"--Utiliser le type 'Person' si l'interprète est un ou des artistes--",
      "@id":"--URI constituant un identifiant unique de l'artiste. Optionnel--",
      "name":"--Nom de l'artiste--",
      "url":"--URL du site officiel de l'artiste--",
      "sameAs":[
         "--lien vers un ou des comptes de réseaux sociaux, si disponible--",
         "--URI Artsdata.ca, obtenu avec l'API de réconciliation Artsdata--",
         "--URI wikidata, si disponible--"]
   }],
   "location":[
         {
            "@type":"Place",
            "@id":"--URI constituant un identifiant unique du lieu. Optionnel--",
            "address":{
               "@type":"PostalAddress",
               "addressLocality":"--Nom de la ville--",
               "addressRegion":"--Nom de la province--",
               "postalCode":"--Code postal--",
               "streetAddress":"--Numéro civique et rue--",
               "addressCountry":"CA"
            },
            "sameAs":[
               "--lien vers un ou des comptes de réseaux sociaux, si disponible--",
               "--URI Artsdata.ca, obtenu avec l'API de réconciliation Artsdata--",
               "--URI wikidata, si disponible--"]
         },{
            "@type":"VirtualLocation",
            "@id":"--URI constituant un identifiant unique de lieu virtuelle. Optionnel--",
            "url":"--URL vers le lieu virtuelle--"
   }],
   "offers":{
      "@type":"Offer",
      "availability":"http://schema.org/InStock",
      "url":"--URL vers la page d'achat de billets--",
      "validFrom":"--Date et heure du début de la disponibilité, au format ISO 8601, par exemple 2016-03-31T08:00:00+08:00--",
      "price":"--Prix au format 9.99--",
      "priceCurrency":"CAD"
   },
   "mainEntityOfPage":[{
      "@type":"WebPage",
      "@id":"--URI constituant un identifiant unique de la page Web. Optionnel--",
      "inLanguage":"fr",
      "url":"URL de la page Web",
      "lastReviewed":"--Date et heure de la dernière révision--"
   }]

}
