adr:ArtsdataEventTypes rdf:type owl:NamedIndividual, skos:ConceptScheme ;
  skos:prefLabel "Controlled Vocabulary for event types" ;
  rdfs:comment "This SKOS Concept Scheme is a controlled vocabulary for event types (types, categories, disciplines) in Artsdata. The goal of this controlled vocabulary is to serve as a base for mapping different arts organization's event types so events can be reused in event calendars." ;
  owl:versionInfo "3.4.5";
  vann:preferredNamespacePrefix "adr" ;
  vann:preferredNamespaceUri adr: ;
  schema:dateCreated "2021-08-10"^^xsd:date ;
  schema:dateModified "2025-02-06"^^xsd:date ;
  schema:creator  adr:K1-5 ;
  vann:usageNote <https://culturecreates.github.io/artsdata-data-model/classes/event.html> ;
  vann:example adr:PerformingArtsEvent ;
  vann:changes <https://github.com/culturecreates/artsdata-data-model/commits/master/ontology/skos-event-types.ttl> ; 
  skos:hasTopConcept adr:Event . 

ado:EventType rdf:type rdfs:Class ; rdfs:label "Artsdata Event types" ; rdfs:comment "Artsdata Event types defined as a class in addition to using SKOS. This Artsdata specific class facilitates reconciliation, instead of reconiling skos:Concepts is it more direct to reconcile individuals that are ado:EventType." .
