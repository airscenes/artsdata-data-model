  vann:preferredNamespaceUri ado: ;
  owl:versionIRI ado: ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  dct:created "2021-08-10"^^xsd:date ;
  dct:modified "2021-11-05"^^xsd:date ;
  schema:creator  adr:K1-5 ;
  vann:usageNote <https://culturecreates.github.io/artsdata-data-model/classes/event.html> ;
  vann:example ado:PerformingArtsEvent ;
  owl:versionInfo "2.0.0";
  vann:changes <https://github.com/culturecreates/artsdata-data-model/commits/master/ontology/skos-event-categories.ttl> ; 
  skos:hasTopConcept ado:Event . 

